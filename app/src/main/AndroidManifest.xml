<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.bilingual_dictionary_id_eng">

    <application
        android:name=".TheApp"
        android:allowBackup="true"
        android:icon="@drawable/app_dictionary"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".primary_ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".primary_ui.dashboard.DashboardActivity"
            android:label="@string/bilingual_dictionary"
            android:theme="@style/AppTheme"/>
    </application>

</manifest>